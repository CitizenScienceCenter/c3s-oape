{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "created_at",
      "description",
      "id",
      "name",
      "owned_by",
      "platform",
      "updated_at"
    ],
    "properties": {
      "created_at": {
        "$id": "#/properties/created_at",
        "type": "string",
        "format": "date-time",
        "title": "The Created_at Schema",
        "default": "",
        "examples": [
          "2015-07-07T15:49:51.230+02:00"
        ],
        "pattern": "^(.*)$"
      },
      "description": {
        "$id": "#/properties/description",
        "type": "string",
        "title": "The Description Schema",
        "default": "",
        "examples": [
          "string"
        ],
        "pattern": "^(.*)$"
      },
      "id": {
        "$id": "#/properties/id",
        "type": "string",
        "title": "The Id Schema",
        "default": "",
        "examples": [
          "string"
        ],
        "pattern": "^(.*)$"
      },
      "name": {
        "$id": "#/properties/name",
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "examples": [
          "string"
        ],
        "pattern": "^(.*)$"
      },
      "owned_by": {
        "$id": "#/properties/owned_by",
        "type": "string",
        "title": "The Owned_by Schema",
        "default": "",
        "examples": [
          "string"
        ],
        "pattern": "^(.*)$"
      },
      "platform": {
        "$id": "#/properties/platform",
        "type": "string",
        "title": "The Platform Schema",
        "default": "",
        "examples": [
          "Desktop"
        ],
        "pattern": "^(.*)$"
      },
      "updated_at": {
        "$id": "#/properties/updated_at",
        "type": "string",
        "title": "The Updated_at Schema",
        "format": "date-time",
        "default": "",
        "examples": [
          "2015-07-07T15:49:51.230+02:00"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
  